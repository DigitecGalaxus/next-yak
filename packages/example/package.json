{
  "name": "next-yak-example",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "pnpm run build:yak && pnpm run build:next",
    "build:next": "next build",
    "build:yak": "pnpm run --filter next-yak build",
    "build:swc": "pnpm run --filter yak-swc build",
    "start": "next start",
    "lint": "next lint",
    "test": "pnpm run test:vitest && pnpm run test:jest",
    "test:vitest": "vitest run",
    "test:jest": "jest",
    "test:watch": "jest --watch -u"
  },
  "dependencies": {
    "@types/node": "20.4.5",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "next": "15.0.4",
    "next-yak": "workspace:*",
    "postcss-nested": "^6.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "typescript": "5.5.2",
    "yak-swc": "workspace:*"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.12",
    "@vitejs/plugin-react": "4.3.1",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jsdom": "^22.1.0",
    "vite": "5.3.4",
    "vitest": "2.0.4"
  }
}
