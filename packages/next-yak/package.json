{
  "name": "next-yak",
  "version": "0.0.5",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./withYak": "./loaders/withYak.cjs"
  },
  "typesVersions": {
    "*": {
      "withYak": [
        "./loaders/withYak.d.ts"
      ],
      "*": [
        "./runtime/index.d.ts"
      ]
    }
  },
  "scripts": {
    "prepublishOnly": "npm run build && npm run test",
    "build": "tsc -p tsconfig.json --outDir dist/",
    "test": "vitest run",
    "test:watch": "vitest --watch -u"
  },
  "dependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-syntax-typescript": "^7.16.7",
    "postcss-nested": "^6.0.1"
  },
  "devDependencies": {
    "@babel/traverse": "^7.16.12",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "29.5.5",
    "@types/node": "20.4.5",
    "@types/react": "18.2.16",
    "@types/react-dom": "18.2.7",
    "jsdom": "^22.1.0",
    "react": "^18.2.0",
    "typescript": "5.1.6",
    "vite": "^4.4.9",
    "vitest": "0.34.5"
  },
  "files": [
    "dist",
    "loaders",
    "runtime"
  ]
